                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dp10">

                                <ImageView
                                    android:id="@+id/img_play"
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:contentDescription="@null"
                                    android:src="@drawable/play" />

                                <Chronometer
                                    android:id="@+id/txt_elaspsed_time"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toStartOf="@+id/img_slash"
                                    android:textColor="@color/txt_color"
                                    android:textSize="@dimen/_12sdp" />

                                <ImageView
                                    android:id="@+id/img_slash"
                                    android:layout_width="10dp"
                                    android:layout_height="20dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginTop="@dimen/dimen_10dp"
                                    android:layout_toStartOf="@+id/txt_time"
                                    android:contentDescription="@null"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/slash" />

                                <com.customview.MyTextView
                                    android:id="@+id/txt_time"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_alignTop="@+id/txt_elaspsed_time"
                                    android:layout_alignBottom="@+id/txt_elaspsed_time"
                                    android:layout_alignParentEnd="true"
                                    android:layout_centerVertical="true"
                                    android:minWidth="50dp"
                                    android:text=""
                                    android:textColor="@color/txt_color"
                                    android:textSize="@dimen/_12sdp" />

                                <SeekBar
                                    android:id="@+id/seekBar"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_alignTop="@+id/txt_elaspsed_time"
                                    android:layout_alignBottom="@+id/txt_elaspsed_time"
                                    android:layout_centerInParent="true"
                                    android:layout_toStartOf="@+id/txt_elaspsed_time"
                                    android:layout_toEndOf="@+id/img_play"
                                    android:src="@drawable/recording_line" />
                            </RelativeLayout>


                            <RelativeLayout
