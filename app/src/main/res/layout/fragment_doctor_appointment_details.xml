                                android:visibility="gone">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:contentDescription="@null"
                                    android:src="@drawable/txt_or_bg" />

                            </RelativeLayout>

                            <com.customview.MyTextView
                                android:id="@+id/tvSymptoms"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:text=""
                                android:textSize="@dimen/_12ssp"
                                android:visibility="gone" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/iv_prescription"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12sdp"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginEnd="@dimen/_12sdp"
                            android:contentDescription="@null"
                            android:scaleType="fitXY"
                            android:src="@drawable/prescription"
                            android:transitionName="@string/transition_string"
                            android:visibility="gone" />

                        <LinearLayout
                            android:id="@+id/ll_prescriptionText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12sdp"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginEnd="@dimen/_12sdp"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <com.customview.MyTextView
                                android:id="@+id/tv_prescriptionStatus"
                                android:layout_width="wrap_content"
